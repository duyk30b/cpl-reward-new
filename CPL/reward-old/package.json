{
  "name": "reward",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/reward/test/jest-e2e.json",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./config/ormconfig.ts",
    "migrate:create": "npm run typeorm:cli -- migration:create -n",
    "migrate:run": "npm run typeorm:cli -- migration:run",
    "migrate:rollback": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.1",
    "@grpc/proto-loader": "^0.6.9",
    "@nestjs/axios": "0.0.5",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.2",
    "@sentry/node": "^6.16.1",
    "@types/dotenv": "^8.2.0",
    "bignumber.js": "^9.1.0",
    "bull": "^4.8.2",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.1",
    "ethers": "^5.6.1",
    "handlebars": "^4.7.7",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.2.9",
    "mysql2": "^2.3.3",
    "nestjs-typeorm-paginate": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/kafka(|/.*)$": "<rootDir>/libs/kafka/src/$1",
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@lib/mission-user(|/.*)$": "<rootDir>/libs/mission-user/src/$1",
      "^@lib/mission(|/.*)$": "<rootDir>/libs/mission/src/$1",
      "^@lib/mission-user-log(|/.*)$": "<rootDir>/libs/mission-user-log/src/$1",
      "^@lib/mysql(|/.*)$": "<rootDir>/libs/mysql/src/$1",
      "^@lib/user(|/.*)$": "<rootDir>/libs/user/src/$1",
      "^@lib/external-user(|/.*)$": "<rootDir>/libs/external-user/src/$1",
      "^@lib/campaign(|/.*)$": "<rootDir>/libs/campaign/src/$1",
      "^@lib/affiliate-internal(|/.*)$": "<rootDir>/libs/affiliate-internal/src/$1",
      "^@lib/reward-rule(|/.*)$": "<rootDir>/libs/reward-rule/src/$1",
      "^@lib/admin-action-log(|/.*)$": "<rootDir>/libs/admin-action-log/src/$1",
      "^@lib/external-balance(|/.*)$": "<rootDir>/libs/external-balance/src/$1",
      "^@lib/mission-event(|/.*)$": "<rootDir>/libs/mission-event/src/$1",
      "^@lib/user-reward-history(|/.*)$": "<rootDir>/libs/user-reward-history/src/$1",
      "^@lib/external-cashback(|/.*)$": "<rootDir>/libs/external-cashback/src/$1",
      "^@lib/mongo(|/.*)$": "<rootDir>/libs/mongo/src/$1",
      "^@lib/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@lib/international-price(|/.*)$": "<rootDir>/libs/international-price/src/$1",
      "^@lib/id-generator(|/.*)$": "<rootDir>/libs/id-generator/src/$1",
      "^@lib/queue(|/.*)$": "<rootDir>/libs/queue/src/$1",
      "^@libs/user-checkin-log(|/.*)$": "<rootDir>/libs/user-checkin-log/src/$1",
      "^@libs/external-new-balance(|/.*)$": "<rootDir>/libs/external-new-balance/src/$1",
      "^@libs/wallet-gateway(|/.*)$": "<rootDir>/libs/wallet-gateway/src/$1",
      "^@libs/new-balance(|/.*)$": "<rootDir>/libs/new-balance/src/$1",
      "^libs/exchange(|/.*)$": "<rootDir>/libs/exchange/src/$1"
    }
  }
}