{
  "name": "starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/dispatcher/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/starter/test/jest-e2e.json",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./config/ormconfig.ts",
    "migrate:create": "npm run typeorm:cli -- migration:create -n",
    "migrate:run": "npm run typeorm:cli -- migration:run",
    "migrate:rollback": "npm run typeorm:cli -- migration:revert",
    "seed:create": "npm run typeorm:cli -- migration:create -c seed -n",
    "seed:run": "npm run typeorm:cli -- migration:run -c seed",
    "seed:rollback": "npm run typeorm:cli -- migration:revert -c seed",
    "command": "node dist/apps/commandline/main"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.236.0",
    "@grpc/grpc-js": "^1.5.3",
    "@grpc/proto-loader": "^0.6.9",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "0.0.1",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.0.7",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@sentry/node": "^6.17.1",
    "@types/cache-manager": "^3.4.2",
    "bull": "^4.4.0",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "ethers": "^5.6.9",
    "firebase-admin": "^10.0.2",
    "ioredis": "^5.2.3",
    "kafkajs": "^1.15.0",
    "mongoose": "^6.3.2",
    "mqtt": "^4.3.7",
    "mysql2": "^2.3.3",
    "nest-commander": "^3.2.0",
    "nestjs-i18n": "^8.2.2",
    "nestjs-typeorm-paginate": "^3.1.3",
    "nodemailer": "^6.7.2",
    "pug": "^3.0.2",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "sharp": "^0.31.3",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.21",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/kafka(|/.*)$": "<rootDir>/libs/kafka/src/$1",
      "^@libs/mail(|/.*)$": "<rootDir>/libs/mail/src/$1",
      "^@libs/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@libs/util(|/.*)$": "<rootDir>/libs/util/src/$1",
      "^@libs/mysql(|/.*)$": "<rootDir>/libs/mysql/src/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/notification(|/.*)$": "<rootDir>/libs/notification/src/$1",
      "^@libs/grpc-client(|/.*)$": "<rootDir>/libs/grpc-client/src/$1",
      "^@libs/auth-kong(|/.*)$": "<rootDir>/libs/auth-kong/src/$1",
      "^@libs/device-token(|/.*)$": "<rootDir>/libs/device-token/src/$1",
      "^@libs/firebase-admin(|/.*)$": "<rootDir>/libs/firebase-admin/src/$1",
      "^@libs/system-push-notification-setting(|/.*)$": "<rootDir>/libs/system-push-notification-setting/src/$1",
      "^@libs/notification-aggregate(|/.*)$": "<rootDir>/libs/notification-aggregate/src/$1",
      "^@libs/user-setting(|/.*)$": "<rootDir>/libs/user-setting/src/$1",
      "^@libs/mongo(|/.*)$": "<rootDir>/libs/mongo/src/$1",
      "^@libs/noti-log(|/.*)$": "<rootDir>/libs/noti-log/src/$1",
      "^@libs/bce-mysql(|/.*)$": "<rootDir>/libs/bce-mysql/src/$1",
      "^@libs/mail-schedule(|/.*)$": "<rootDir>/libs/mail-schedule/src/$1",
      "^@libs/push-schedule(|/.*)$": "<rootDir>/libs/push-schedule/src/$1",
      "^@libs/websocket(|/.*)$": "<rootDir>/libs/websocket/src/$1",
      "^@libs/user-group(|/.*)$": "<rootDir>/libs/user-group/src/$1",
      "^@libs/upload-file(|/.*)$": "<rootDir>/libs/upload-file/src/$1"
    }
  }
}
